{
  "name": "@elderbyte/ngx-stomp",
  "version": "0.0.1",
  "description": "Stomp client for Angular Typescript.",
  "main": "dist/bundles/ngx-stomp.umd.js",
  "module": "dist/index.js",
  "scripts": {
    "cleanup": "rimraf dist/bundles dist/src dist/index.d.ts dist/index.metadata.json dist/index.js dist/index.js.map dist/LICENSE dist/README.md",
    "bundling": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-stomp.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-stomp.umd.min.js",
    "copy": "copyfiles LICENSE README.md dist",
    "build": "npm run cleanup && ngc && npm run bundling && npm run minify && npm run copy"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular 2",
    "stomp",
    "web sockets"
  ],
  "author": "Elderbyte",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ElderByte-/ngx-stomp.git"
  },
  "homepage": "https://github.com/ElderByte-/ngx-stomp",
  "bugs": {
    "url": "https://github.com/ElderByte-/ngx-stomp/issues"
  },
  "dependencies": {
    "@angular/core": "^4.1.3",
    "reflect-metadata": "~0.1.10",
    "rxjs": "^5.3.0",
    "zone.js": "~0.8.5"
  },
  "devDependencies": {
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "copyfiles": "^1.2.0",
    "rimraf": "^2.6.1",
    "rollup": "~0.41.6",
    "typescript": "^2.3.0",
    "uglify-js": "^2.8.20"
  }
}
